if not os.user then
print("Not logged in.")
return
end
if os.user == "guest" then
print("Cannot run as guest.")
return
end
print("You are about to delete your account on this system.")
print("Please enter your password to confim.")
local h=fs.open("/secacct/"..os.user.."/.password", "r")
if read("*")==h.readAll() then
h.close()
local h = fs.open("/secacct/queue", "a")
h.writeLine(os.user)
h.close()
print("Sucess.")
sleep(2)
os.reboot()
else
print("Password is incorrect.")
end
